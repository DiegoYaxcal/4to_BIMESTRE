#include <iostream>

using namespace std;

int main() {
  // Declara una matriz de 3x3
  int matriz[3][3];

  // Solicita al usuario que ingrese los valores de la matriz
  for (int i = 0; i < 3; i++) {
    for (int j = 0; j < 3; j++) {
      cout << "Ingrese el valor para la posicion [" << i << "," << j << "]: ";
      cin >> matriz[i][j];
    }
  }

  // Imprime la diagonal principal
  cout << "La diagonal principal es: ";
  for (int i = 0; i < 3; i++) {
    cout << matriz[i][i] << " ";
  }
  cout << endl;

  return 0;
}
